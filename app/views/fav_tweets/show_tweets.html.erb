<div class="time_line">
	<%= @home_timeline.each do |tweet| %>

	<div class="tweet"> 
		<div class="user_name"> <%= tweet.user.user_name %> </div>
		<div class="text"> <%= tweet.text %> </div>
		<%= button_to "Favorite", create_fav_tweet_path(tweet), method: 'post' %>
		<%= button_to "Show", display_fav_tweet_path(tweet.user.user_name), method: 'get' %>
	</div>

	<% end %>
</div>

